package com.gmail.terry996996.dao;

import java.util.Optional;

import org.springframework.data.jpa.repository.JpaRepository;

import com.gmail.terry996996.domain.UserBean;

public interface UserDAO extends JpaRepository<UserBean, Integer>{

	 Optional<UserBean> findByAccountAndPassword(String account, String password);
	 Optional<UserBean> findByAccount(String account);
	 

}